<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <title>@ViewBag.Title</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">

        <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
        <link rel="stylesheet" type="text/css"  href="@Url.Content("~/Content/css/kendo.common.min.css")">
        <link rel="stylesheet" type="text/css"  href="@Url.Content("~/Content/css/kendo.default.min.css")">
        <link rel="stylesheet" type="text/css"  href="@Url.Content("~/Content/css/slick.grid.css")">
        
        <link rel="stylesheet" type="text/css"  href="@Url.Content("~/Content/css/jquery.multiselect.css")">
        <link rel="stylesheet" type="text/css"  href="@Url.Content("~/Content/css/jquery.multiselect.filter.css")">

        <link rel="stylesheet/less" type="text/css" href="@Url.Content("~/Content/css/mvc.boilerplate/mvc.boilerplate.less")">

        <script src="@Url.Content("~/Content/scripts/lib/modernizr-2.6.1.min.js")"></script>
        
        <script src="@Url.Content("~/Content/scripts/lib/less-1.3.0.min.js")"></script>

        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
        <script> window.jQuery || document.write('<script src="@Url.Content("~/Content/scripts/lib/jquery-1.8.0.min.js")"><\/script>')</script>
        <script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.9.1/jquery-ui.min.js"></script>
        <script> (jQuery.ui) || document.write('<script src="@Url.Content("~/Content/scripts/lib/jquery-ui-1.9.1.min.js")"><\/script>')</script>

        <script src="@Url.Content("~/Content/scripts/lib/kendo.web.min.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/prettify.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/jquery.event.drag-2.0.min.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/slick.core.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/slick.grid.js")"></script>

        <script src="@Url.Content("~/Content/scripts/lib/jquery.multiselect.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/jquery.multiselect.filter.js")"></script>

        <script src="@Url.Content("~/Content/scripts/lib/jquery.splitbutton.js")"></script>

        <script src="@Url.Content("~/Content/scripts/lib/html.encode.decode.js")"></script>
        <script src="@Url.Content("~/Content/scripts/lib/mvc-boilerplate.utility.js")"></script>
        
        
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-transition.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-alert.js")"></script> 
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-modal.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-dropdown.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-scrollspy.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-tab.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-tooltip.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-popover.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-button.js")"></script> 
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-collapse.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-carousel.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-typeahead.js")"></script>
        <script src="@Url.Content("~/Content/scripts/bootstrap/bootstrap-affix.js")"></script> 

        <script src="@Url.Content("~/Content/scripts/boilerplate/slickgrid-initialiser.js")"></script>
        
        <script src="@Url.Content("~/Content/scripts/boilerplate-devguide/application.js")"></script>

        
        
    </head>
    <body class="style-guide" data-spy="scroll" data-target=".side-navigation" data-twttr-rendered="true">
        <!--[if lt IE 7]>
            <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
        <![endif]-->

        @Html.Partial("Layout/_Navbar")
        @Html.Partial("Layout/_Header")

        <!-- Add your site or application content here -->
        <div class="container">
            <div class="row">
                <div class="@Html.AttributeEncode((IsSectionDefined("SideNavigation")) ? "span3 side-navigation" : "ui-helper-hidden")">
                    @RenderSection("SideNavigation", false)
                </div>

                <div class="@Html.AttributeEncode((IsSectionDefined("SideNavigation")) ? "span9 " : "span12 ")content-panel">
                    @RenderBody()
                </div>
            </div>
        </div>

        @if (ViewBag.UseGoogleAnalytics)
        {
            @Html.Partial("Layout/_GoogleAnalytics")
        }
    </body>
</html>
